create database insurance;
use insurance;
create table person(
driverid varchar(20),pname char(20),address char(50),
primary key (driverid));
create table car(
regno char(10), model char(20), location char(20),primary key(regno));
alter table car
drop column location;
 alter table car
add column year int;
create table accident(
reportno int(10), date int(10),location char(30), primary key(reportno));
create table owns(
driverid varchar(20), regno char(10), primary key(driverid,regno), 
foreign key (regno) references car(regno),foreign key (driverid) references person(driverid));
create table participated(
driverid varchar(20),regno char(10),reportno int(10),damageamt int(20),primary key(driverid,regno,reportno),
foreign key (driverid) references person(driverid),foreign key (regno) references car(regno),
foreign key (reportno) references accident(reportno));
